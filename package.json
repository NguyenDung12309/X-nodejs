  "scripts": {
    "dev": "npx tsx --watch --experimental-specifier-resolution=node ./src/index.ts",
    "build": "env-cmd -f .env.production rimraf ./dist && tsc && tsc-alias",
    "start": "env-cmd -f .env.production node dist/index.js",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "lint-parse": "npx @eslint/migrate-config .eslintrc.json",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "lint:js": "eslint . --ignore-pattern .gitignore",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/i18n": "^0.13.12",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "express": "^4.19.2",
    "formidable": "^3.5.1",
    "i18n": "^0.15.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongodb": "^6.8.0",
    "sharp": "^0.33.5"
  }
